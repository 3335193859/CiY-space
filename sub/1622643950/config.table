编辑框.append(\"\\n请在文字下面输入对应数字，回车出拳！\\n规则:(1.剪刀 2.石头 3.布)\\n\");\
function 开始(a)\
  local b=(Math.random()*3)+1;\
  local b=(tointeger(b))\
  local c =\"拳头\";\
  local d =\"拳头\";\
  if a==1 then\
    c=\"剪刀\";\
   elseif a==2 then\
    c=\"石头\";\
   elseif a==3 then\
    c=\"布\";\
  end\
  if b==1 then\
    d=\"剪刀\";\
   elseif b==2 then\
    d=\"石头\";\
   elseif b== 3 then\
    d=\"布\";\
  end\
  if(a==b) then\
    编辑框.append(\"\\n (●—●)平局~\"..\"\\n你:\"..c..\"机器人\\n:\"..d);\
   elseif(a==1 and b==2 or a==2 and b==3 or a==3 and b==1) then\
    编辑框.append(\"\\n (ง •̀_•́)ง你输了~\"..\"\\n你:\"..c..\"\\n机器人:\"..d);\
   else\
    编辑框.append(\"\\n 乾杯 []~（￣▽￣）~*恭喜你，你赢了~\"..\"\\n你:\"..c..\"\\n机器人:\"..d);\
  end\
end\
编辑框.setOnEditorActionListener{\
  onEditorAction=function(v, actionId, event)\
    n=tonumber(编辑框.getText().substring(#编辑框.getText()-1,#编辑框.getText()))\
    if type(n)==\"number\" then\
      if n==1 or n==2 or n==3 then\
        开始(n)\
       else\
        编辑框.append(\"\\n 不合法的输入\\n\");\
      end\
     else\
      编辑框.append(\"\\n只能输入数字！\\n\");\
    end\
    return false\
  end}\
编辑框.getPaint().setFakeBoldText(true)\
function 三色渐变(控件,左色,中色,右色,圆角)\
  import \"android.graphics.drawable.GradientDrawable\"\
  drawable = GradientDrawable(GradientDrawable.Orientation.TR_BL,{左色,中色,右色});\
  drawable.setCornerRadii({圆角,圆角,圆角,圆角,圆角,圆角,圆角,圆角});\
  控件.setBackgroundDrawable(drawable)\
end\
